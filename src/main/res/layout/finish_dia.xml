<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="251.5dp"
    android:layout_height="235dp"
    android:background="@drawable/twopop"
    android:layout_gravity="center_horizontal"
    android:gravity="center_horizontal">

    <TextView
        android:layout_width="188.75dp"
        android:layout_height="wrap_content"
        android:textAlignment="center"
        android:layout_marginTop="41.5dp"
        android:text="산책을\n정말 마치시겠어요?"
        android:textColor="#727272"
        android:gravity="center"
        android:textSize="17sp"
        android:id="@+id/textView8"
        android:layout_gravity="center_horizontal" />

    <Button
        android:layout_width="127.4dp"
        android:layout_height="38.8dp"
        android:layout_marginTop="48dp"
        android:id="@+id/btn_finish_no"
        android:background="@drawable/btncontinue"
        android:layout_gravity="center_horizontal" />

    <Button
        android:layout_width="123.54dp"
        android:layout_height="37.56dp"
        android:background="@drawable/fin"
        android:layout_marginTop="6dp"
        android:id="@+id/btn_finish_yes"
        android:layout_gravity="center_horizontal" />
</LinearLayout>