<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="horizontal"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:gravity="center_vertical"
    android:paddingLeft="43.825dp">
    <CheckBox

        android:layout_height="16.3dp"
        android:layout_width="16.3dp"
        android:id="@+id/main_check_box"
        android:button="@null"
        android:background="@drawable/custom_checkbox"
        android:layout_marginRight="23.25dp"
       />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="44.4dp"
        android:textSize="17sp"
        android:gravity="center"
        android:id="@+id/main_check_text"
        android:textColor="#919191"
        android:text="지역구"/>



</LinearLayout>